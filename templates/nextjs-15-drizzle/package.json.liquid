{
  "name": "{{ appName }}",
  "description": "{{ description }}",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev --turbopack",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build"{% if withDrizzle %},
    "db:generate": "drizzle-kit generate",
    "db:migrate": "tsx scripts/migrate.ts",
    "db:push": "drizzle-kit push",
    "db:studio": "drizzle-kit studio"{% endif %}
  },
  "dependencies": {
    "clsx": "^2.0.0",
    "lucide-react": "^0.438.0",
    "next": "^15.5.4",
    "react": "^19.1.1",
    "react-dom": "^19.1.1",
    "react-icons": "^5.5.0",
    "tailwind-merge": "^2.6.0"{% if withDrizzle %},
    "drizzle-orm": "^0.36.4",{% if databaseProvider == 'neon' %}
    "@neondatabase/serverless": "^0.10.3"{% elsif databaseProvider == 'vercel-postgres' %}
    "@vercel/postgres": "^0.10.0"{% elsif databaseProvider == 'supabase' or databaseProvider == 'local-postgres' %}
    "postgres": "^3.4.5"{% endif %}{% endif %}
  },
  "devDependencies": {
    "@storybook/addon-links": "^9.1.10",
    "@storybook/cli": "^9.1.10",
    "@storybook/nextjs": "^9.1.10",
    "@storybook/react": "^9.1.10",
    "@tailwindcss/forms": "^0.5.10",
    "@tailwindcss/postcss": "^4.1.14",
    "@types/node": "^22.0.0",
    "@types/react": "^19.1.17",
    "@types/react-dom": "^19.1.17",
    "autoprefixer": "^10.4.20",{% if withDrizzle %}
    "drizzle-kit": "^0.28.1",
    "dotenv": "^16.4.7",
    "tsx": "^4.20.6",{% endif %}
    "eslint": "^9.36.0",
    "eslint-config-next": "^15.5.4",
    "eslint-plugin-storybook": "^0.11.1",
    "postcss": "^8.5.6",
    "tailwindcss": "^4.1.14",
    "typescript": "^5.9.3"
  }
}